msgctxt "model:ir.message,text:msg_amount_invoices_greater_amount"
msgid ""
"You can not post the voucher because Invoices amount is greater than Payment"
" amount"
msgstr ""
"No puede confirmar el comprobante porque el importe de facturas es mayor al "
"importe de pago"

msgctxt "model:ir.message,text:msg_change_voucher_sequence"
msgid ""
"You can not change voucher sequence in fiscal year \"%(fiscal_year)s\" "
"because there are already posted vouchers in this fiscal year."
msgstr ""
"No puede cambiar la secuencia del comprobante en el año fiscal "
"«%(fiscal_year)s» porque ya hay comprobantes confirmados en este año fiscal."

msgctxt "model:ir.message,text:msg_check_not_in_draft"
msgid "Check \"%(check)s\" is not in draft state"
msgstr "El cheque número \"%(check)s\" no está en estado borrador"

msgctxt "model:ir.message,text:msg_delete_voucher"
msgid "You can not delete a voucher that is posted!"
msgstr "¡No puede eliminar un comprobante ya confirmado!"

msgctxt "model:ir.message,text:msg_different_voucher_sequence"
msgid "Fiscal year \"%(first)s\" and \"%(second)s\" have the same voucher sequence."
msgstr ""
"El año fiscal «%(first)s» y «%(second)s» tienen la misma secuencia de "
"comprobante."

msgctxt "model:ir.message,text:msg_issued_check_not_issued"
msgid "Issued check \"%(check)s\" is not in Issued state"
msgstr "El cheque emitido número \"%(check)s\" no figura como \"Emitido\""

msgctxt "model:ir.message,text:msg_missing_pay_lines"
msgid "You have to enter pay mode lines!"
msgstr "¡Debe ingresar las líneas de formas de pago!"

msgctxt "model:ir.message,text:msg_no_journal_check_account"
msgid "You need to define a check account in the journal \"%(journal)s\""
msgstr "Debe definir la cuenta del cheque en el diario \"%(journal)s\""

msgctxt "model:ir.message,text:msg_post_already_reconciled"
msgid ""
"You can not post the voucher because it already has reconciled "
"lines!\\n\\nLines:\\n%(lines)s"
msgstr "No puede confirmar el comprobante porque ya posee líneas conciliadas!"

msgctxt "model:ir.message,text:msg_refund_with_amount_difference"
msgid ""
"You can not credit with refund invoice \"%(invoice)s\" because total amount "
"is different than amount to pay."
msgstr ""
"No puede abonar con devolución la factura \"%(invoice)s\" porque el total es"
" diferente al importe a pagar."

msgctxt "model:ir.message,text:msg_third_check_not_held"
msgid "Third check \"%(check)s\" is not in Held state"
msgstr "El cheque de tercero número \"%(check)s\" no figura como \"En cartera\""

msgctxt "model:ir.message,text:msg_third_pay_check_not_delivered"
msgid "Third check \"%(check)s\" is not in Delivered state"
msgstr "El cheque de tercero número \"%(check)s\" no figura como \"Entregado\""

